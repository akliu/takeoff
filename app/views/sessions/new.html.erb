<h1 class="title">Takeoff</h1>
<br>
<h3 class="sign-up-in-form">Sign In</h3>

<form class="sign-up-in-form" action="<%= session_url %>" method="post">
  <%= auth_token_input %>
  <input type="hidden" name="newSession" value="true" >

  <label>
    Username
    <input type="text" name="user[username]" value="">
  </label>

  <label>
    Password
    <input type="password" name="user[password]" value="">
  </label>

  <input type="submit" value="Sign In">
  <br>
  <br>
  <p>Not a Member?
    <%= link_to "Sign Up here!", new_user_url %>
  </p>
</form>
