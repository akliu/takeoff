<h1 class="title">Takeoff</h1>
<br>
<h3 class="sign-up-in-form">Sign Up</h3>

<form class="sign-up-in-form" action="<%= users_url %>" method="post">
  <%= auth_token_input %>
  <label>
    Username
    <input type="text" name="user[username]" value="">
  </label>

  <label>
    Password
    <input type="password" name="user[password]" value="">
  </label>

  <input type="submit" value="Sign Up">
  <br>
  <br>
  <p>Already a flyer?
    <%= link_to "Sign in here!", new_session_url %>
  </p>
</form>
